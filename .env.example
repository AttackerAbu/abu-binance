# Rename to .env and fill values
PORT=8080
NODE_ENV=production

# Basic auth for dashboard routes (optional)
BASIC_USER=admin
BASIC_PASS=change-me

# Binance settings
BINANCE_TESTNET=true
BINANCE_API_KEY=your_key
BINANCE_API_SECRET=your_secret

# Allowed CORS origin for your frontend (e.g. https://bot.cabcompare.in)
CORS_ORIGIN=*
